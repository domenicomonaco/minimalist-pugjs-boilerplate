{
    "title": "MONK Boilerplate",
    "name": "minimalist-pugjs-boilerplate",
    "version": "1.0.0",
    "scripts": {
        "build": "npm run clean && npm run build:static && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
        "build:assets": "node scripts/build-assets.js",
        "build:pug": "node scripts/build-pug.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "build:static": "node scripts/build-static.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "npm run build && node scripts/start-debug.js"
    },
    "description": "Creative is a stylish one page HTML theme built with Bootstrap.",
    "keywords": [
        "css",
        "sass",
        "scss",
        "html",
        "pug",
        "responsive",
        "theme",
        "template",
        "boilerplate",
        "static"
    ],
    "homepage": "https://",
    "bugs": {
        "url": "https://github.com/domenicomonaco/minimalist-pugjs-boilerplate/issues",
        "email": "domenico@tecnologieperpersone.it"
    },
    "license": "MIT",
    "author": "Domenico Monaco (https://domenicomonaco.it)",
    "contributors": [
        "Tecnologie per Persone (https://tecnologieperpersone.it)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/domenicomonaco/minimalist-pugjs-boilerplate.git"
    },
    "dependencies": {
        "bootstrap": "5.1.3"
    },
    "devDependencies": {
        "autoprefixer": "10.3.7",
        "browser-sync": "2.27.5",
        "chokidar": "3.5.2",
        "concurrently": "6.3.0",
        "postcss": "8.3.9",
        "prettier": "2.4.1",
        "pug": "3.0.2",
        "sass": "1.43.2",
        "shelljs": "0.8.4",
        "upath": "2.0.1"
    }
}
